<% content_for :title, "Edit Feedback" %>
<div class="container mt-5 mb-5">
  <h1>Edit Feedback</h1>
  <%= render "form", feedback: @feedback %>
  <div>
    <%= link_to "Show this feedback", @feedback, {class: "btn btn-info me-2"} %>
    <%= link_to "Back to list feedback", "/feedbacks?id_user=#{session[:user_id]}", {class: "btn btn-secondary"} %>
  </div>
</div>
