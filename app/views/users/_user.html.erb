<div id="<%= dom_id user %>">
  <div class="row justify-content-center mt-sm-5">
    <div class="col-sm-12">
      <div class="card-client hover-zoom-effect">
        <p>Email: <%= user.email %></p>
        <% role = ['Admin','Freelancer', 'Client'] %>
        <p>Role: <%= role[user.role] %></p>
      </div>
    </div>
  </div>
</div>
