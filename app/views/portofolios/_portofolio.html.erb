<div id="<%= dom_id portofolio %>">
  <h5 class="card-title mb-3"><%= portofolio.title %></h5>
  <hr>
  <div>
    <h5 class="card-label">Description</h5>
    <p class="card-text"><%= portofolio.description %></p>
  </div>
  <div>
    <h5 class="card-label">Client Name</h5>
    <p class="card-text">
      <% if portofolio.client_name.blank? %>
        -
      <% else %>
        <%= portofolio.client_name %>
      <% end %>
    </p>
  </div>
  <div>
    <h5 class="card-label">Client Industry</h5>
    <p class="card-text">
      <% if portofolio.client_industry.blank? %>
        -
      <% else %>
        <%= portofolio.client_industry %>
      <% end %>
    </p>
  </div>
  <div>
    <h5 class="card-label">Url Portofolio</h5>
    <p class="card-text">
      <% if portofolio.link_url.blank? %>
        -
      <% else %>
        <%= link_to "Visit Page", portofolio.link_url, {class: 'card-link'} %>
      <% end %>
    </p>
  </div>
  <%= link_to "See Attachment", portofolio.porto_attachment_url, {class: 'mt-3 btn btn-outline-secondary'} %>
</div>
